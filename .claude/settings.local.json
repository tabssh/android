{
  "permissions": {
    "allow": [
      "Read(**)",
      "Write(**)",
      "Bash(:*:*)",
      "Bash(docker run:*)",
      "Bash(make dev:*)",
      "Bash(tee:*)",
      "Bash(./build.sh:*)",
      "Bash(docker ps:*)",
      "Bash(ls:*)",
      "Bash(aapt dump:*)",
      "Bash(md5sum:*)",
      "Bash(unzip:*)",
      "Bash(make build:*)",
      "Bash(cat:*)",
      "Bash(python3:*)",
      "Bash(./gradlew :app:generateDebugBuildConfig:*)"
    ],
    "deny": [
      "Bash(git commit:*)",
      "Bash(git commit :*)"
    ]
  },
  "model": "sonnet",
  "preferences": {
    "auto_commit": false
  }
}
